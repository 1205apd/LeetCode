class Solution:
    def kidsWithCandies(self, candies: List[int], extraCandies: int) -> List[bool]:
        count=0
        a=max(candies)
        res=[]
        for i in candies:
            if i+extraCandies>=a:
                res.append(True)
            else:
                res.append(False)
        return res
                
        
